<!-- 新的：接口对其了H5 -->
<canvas
        wx:if="{{isUseNewCanvas}}"
        type="2d"
        class="ec-canvas"
        canvas-id="{{ canvasId }}"
        force-use-old-canvas="true"
        bind:init="init"
        bind:touchstart="{{ ec.disableTouch ? '' : 'touchStart' }}"
        bind:touchmove="{{ ec.disableTouch ? '' : 'touchMove' }}"
        bind:touchend="{{ ec.disableTouch ? '' : 'touchEnd' }}"></canvas>
<!-- 旧的 -->
<canvas
        wx:else
        class="ec-canvas"
        canvas-id="{{ canvasId }}"
        bind:init="init"
        bind:touchstart="{{ ec.disableTouch ? '' : 'touchStart' }}"
        bind:touchmove="{{ ec.disableTouch ? '' : 'touchMove' }}"
        bind:touchend="{{ ec.disableTouch ? '' : 'touchEnd' }}"></canvas>

