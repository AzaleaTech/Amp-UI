<view class="example-box">
  <view class="example-box-title">标签大小</view>
  <view class="example-box-content">
    <a-tag
           wx:for="{{ tagList1 }}"
           wx:key="index"
           name="{{ item }}"
           size="{{ item }}"
           class="tag-box"></a-tag>
  </view>
  <view class="example-box-title">标签类型</view>
  <view class="example-box-content">
    <a-tag
           wx:for="{{ tagList2 }}"
           wx:key="index"
           name="{{ item }}"
           type="{{ item }}"
           class="tag-box"></a-tag>
  </view>
  <view class="example-box-title">镂空标签</view>
  <view class="example-box-content">
    <a-tag
           wx:for="{{ tagList2 }}"
           wx:key="index"
           plain
           name="{{ item }}"
           type="{{ item }}"
           class="tag-box"></a-tag>
  </view>
  <view class="example-box-title">圆角标签</view>
  <view class="example-box-content">
    <a-tag
           wx:for="{{ tagList2 }}"
           wx:key="index"
           circle
           name="{{ item }}"
           type="{{ item }}"
           class="tag-box"></a-tag>
  </view>
  <view class="example-box-title">禁用标签</view>
  <view class="example-box-content">
    <a-tag
           wx:for="{{ tagList2 }}"
           wx:key="index"
           disabled
           name="{{ item }}"
           type="{{ item }}"
           class="tag-box"></a-tag>
  </view>
</view>

