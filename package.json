{
  "name": "amp-ui",
  "version": "2.4.0",
  "description": "微信小程序组件库",
  "main": "app.js",
  "private": true,
  "devDependencies": {
    "@babel/core": "^7.15.5",
    "@babel/preset-env": "^7.15.6",
    "del": "^6.0.0",
    "eslint": "^7.22.0",
    "eslint-config-airbnb": "^18.2.1",
    "eslint-config-prettier": "^8.1.0",
    "eslint-plugin-prettier": "^3.3.1",
    "lerna": "^5.1.8",
    "prettier": "^2.2.1"
  },
  "scripts": {
    "install": "npx lerna bootstrap",
    "dev": "npx lerna run dev --scope amp-ui",
    "icon": "npx lerna run iconfont-wechat --scope amp-ui",
    "ci-publish": "npx lerna run ci-publish --scope amp-ui",
    "docs:dev": "npx lerna run docs:dev --scope amp-ui-docs",
    "docs:build": "npx lerna run docs:build --scope amp-ui-docs",
    "clean": "npx lerna clean"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/AzaleaTech/Amp-UI"
  },
  "keywords": [
    "微信小程序",
    "less",
    "组件"
  ],
  "author": "saxon",
  "license": "MIT",
  "workspaces": [
    "packages/*"
  ]
}
