.a-empty {
  box-sizing: border-box;
  padding: 64rpx 0;
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: center;
}
.a-empty-image {
  width: 320rpx;
  height: 320rpx;
}
.a-empty-image-content {
  width: 100%;
  height: 100%;
}
.a-empty-description {
  margin-top: 32rpx;
  padding: 0 120rpx;
  line-height: 40rpx;
  font-size: 28rpx;
  color: #969799;
}
.a-empty-footer {
  margin-top: 48rpx;
}
