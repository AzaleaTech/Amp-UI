page {
  padding-bottom: 30rpx;
  background-color: #f4f5f6;
}
.example-box {
  margin-bottom: 20rpx;
}
.example-box-title {
  margin: 20rpx 30rpx;
  color: #2f2f2f;
  font-weight: 700;
}
.example-box-content {
  padding: 20rpx 30rpx;
  background-color: #ffffff;
}
a-swiper .a-swiper-class{
  height: 340rpx;
}

.example-box-section{
  width: 100%;
  margin-bottom: 60rpx;
}
.example-box-section-first{
  margin-top: 40rpx;
  margin-bottom: 0;
}
.example-box-section:last-child{
  margin-bottom: 0;
}
.example-box-section-spacing{
  box-sizing: border-box;
  padding: 0 80rpx;
}
.example-box-section-title{
  font-size: 28rpx;
  color: #999999;
  margin-bottom: 10rpx;
  margin-top: 60rpx;
  position: relative;
}
.info{
  position: absolute;
  right: 0;
  color: #353535;
  font-size: 30rpx;
}
.section-cells {
  position: relative;
  margin-top: 1.17647059em;
  background-color: #FFFFFF;
  line-height: 1.41176471;
  font-size: 17px;
}
.section-cells:before {
  content: " ";
  position: absolute;
  left: 0;
  top: 0;
  right: 0;
  height: 1px;
  border-top: 1rpx solid #D9D9D9;
  color: #D9D9D9;
}
.section-cells:after {
  content: " ";
  position: absolute;
  left: 0;
  bottom: 0;
  right: 0;
  height: 1px;
  border-bottom: 1rpx solid #D9D9D9;
  color: #D9D9D9;
}
.section-cells-after-title {
  margin-top: 0;
}
.section-cell {
  padding: 10px 15px;
  position: relative;
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  -webkit-box-align: center;
  -webkit-align-items: center;
          align-items: center;
}
.section-cell:before {
  content: " ";
  position: absolute;
  left: 0;
  top: 0;
  right: 0;
  height: 1px;
  border-top: 1rpx solid #D9D9D9;
  color: #D9D9D9;
}
.section-cell:first-child:before {
  display: none;
}
.section-cell-bd {
  -webkit-box-flex: 1;
  -webkit-flex: 1;
          flex: 1;
}
.section-cell-ft {
  text-align: right;
  color: #999999;
}
.section-cell-switch {
  padding-top: 6px;
  padding-bottom: 6px;
}
