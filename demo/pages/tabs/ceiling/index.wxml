<view class="example-box">
  <view class="example-box-title">顶部吸附</view>
  <a-tabs
          tabList="{{ staticTabs }}"
          current="{{ currentTab }}"
          ceiling="{{ ceiling }}">
    <a-card
            wx:for="{{ [0,0,0,0,0,0] }}"
            wx:key="index"
            title="{{ card.title }}"
            contents="{{ card.contents }}"
            imgUrl="{{ card.imgUrl }}"></a-card>
  </a-tabs>
</view>

